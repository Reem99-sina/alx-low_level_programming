#include "main.h"
#include <string.h>
#include <stdio.h>

/**
 *_strncat -  concatenates two strings
 *@dest: first string
 *@src:second string
 *@n:number
 *Return:the resulting string dest
*/
char *_strncat(char *dest, char *src, int n)
{
int i;
char *result;
strcat(dest, src);
for (i = 0; i < n; i++)
{
result[i] = dest[i];
}
return (result);
}
