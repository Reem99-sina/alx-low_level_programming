#include <stdio.h>
#include <stdlib.h>

/**
* main - the result of the multiplication
*@argc: number of arguments
*@argv:array of arguments
*Return: the number of arguments
*/

int main(int argc, char *argv[])
{
if (argv[1] && argv[2])
{
printf("%d\n", atoi(argv[1]) * atoi(argv[2]));
}
else
{
printf("Error\n");
}
return (0);
}
